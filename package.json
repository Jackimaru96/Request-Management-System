{
  "name": "TMS 2.0",
  "version": "0.1.0",
  "scripts": {
    "build:prod": "webpack --config webpack.prod.ts --stats-error-details",
    "build:staging": "webpack --config webpack.staging.ts --stats-error-details",
    "lint:megalint": "npx mega-linter-runner -d stratcomms.common.infopc.net/ci-runners/megalinter-javascript",
    "lint:fix": "eslint src/**/* --fix",
    "lint": "eslint src/**/* && tsc --noEmit",
    "lint:eslint": "eslint src/**/*",
    "lint:ci": "eslint src && tsc --noEmit",
    "lint:ts": "tsc --noEmit",
    "start:prod": "webpack serve --config webpack.prod.ts",
    "start:staging": "webpack serve --config webpack.staging.ts",
    "start:dev": "webpack serve --config webpack.dev.ts",
    "format": "prettier --write '**/**/*.{js,ts,jsx,tsx,cjs,json,md}' --config ./.prettierrc.json"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/js": "^9.39.1",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "babel-loader": "^9.2.1",
    "css-loader": "^7.1.2",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^15.15.0",
    "html-webpack-plugin": "^5.6.4",
    "prettier": "3.6.2",
    "style-loader": "^4.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.1",
    "webpack-merge": "^6.0.1"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.5",
    "@mui/material": "^7.3.5",
    "@tanstack/react-query": "^5.62.18",
    "axios": "^1.13.2",
    "react": "^19.2.3",
    "react-dom": "^19.2.0",
    "react-router": "^7.6.2",
    "@nautilus/nds-react": "file:./nds-react-1.2.3.tgz"
  }
}
